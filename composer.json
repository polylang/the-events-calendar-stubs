{
	"name": "wpsyntex/the-events-calendar-stubs",
	"description": "The Events Calendar function and class declaration stubs for static analysis.",
	"license": "GPL-3.0-or-later",
	"homepage": "https://github.com/polylang/the-events-calendar-stubs",
	"type": "library",
	"repositories": [
		{
			"type": "composer",
